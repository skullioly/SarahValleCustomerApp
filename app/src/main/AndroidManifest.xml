<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.bignerdranch.com.sarahvallecustomerapp">


    <uses-permission-sdk-23 android:name="android.permission.CAMERA" />
    <uses-permission-sdk-23 android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CustomerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CompleteSessionActivity" />
        <activity android:name=".DisplayActivity" />
        <activity android:name=".NewActivity" />
        <activity android:name=".NewSessionActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".SessionActivity" />
        <activity android:name=".ViewListContents" />
        <activity android:name=".fragment_logoff" />



    </application>
    <uses-feature android:name="android.hardware.camera"
        android:required="true" />

</manifest>